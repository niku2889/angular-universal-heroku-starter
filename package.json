{
  "name": "angular-universal-heroku",
  "version": "0.0.0",
  "engines": {
    "node": "9.2.1",
    "yarn": "1.5.1"
  },
  "scripts": {
    "start:heroku": "node dist/server",
    "heroku-postbuild": "npm run build:ssr",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint --fix",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-universal-heroku:server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.0",
    "@angular/common": "7.2.0",
    "@angular/compiler": "7.2.0",
    "@angular/core": "7.2.0",
    "@angular/forms": "7.2.0",
    "@angular/http": "7.2.0",
    "@angular/platform-browser": "7.2.0",
    "@angular/platform-browser-dynamic": "7.2.0",
    "@angular/platform-server": "7.2.0",
    "@angular/pwa": "^0.12.0",
    "@angular/router": "7.2.0",
    "@angular/service-worker": "7.2.0",
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-angular": "7.1.2",
    "@ng-toolkit/universal": "1.1.47",
    "@nguniversal/express-engine": "7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "7.0.2",
    "core-js": "2.5.7",
    "cors": "2.8.5",
    "express": "4.15.2",
    "husky": "1.3.1",
    "rxjs": "6.3.3",
    "ts-loader": "4.2.0",
    "webpack": "4.0.0",
    "webpack-cli": "2.1.5",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.4",
    "@angular/cli": "7.1.4",
    "@angular/compiler-cli": "7.2.0",
    "@angular/language-service": "7.2.0",
    "@types/jasmine": "2.8.14",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "8.10.39",
    "codelyzer": "4.5.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.4.1",
    "ts-node": "7.0.1",
    "tslint": "5.12.0",
    "typescript": ">=3.1.1 <3.2"
  }
}
